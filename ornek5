


#include <stdio.h>

int main(int argc, const char * argv[]) {
    int mesai;
    printf("l端tfen mesai saatinizi giriniz");
    scanf("%d",&mesai);
    if(mesai<10){
        printf("端cret :%d ",mesai);
        
    }else if(mesai>10 && mesai<20){
        printf("端cret :%d",10*5+(mesai-10)*3);
    
        
    }else{
        printf("端cret=%d",10*5+10*3+(mesai-20)*3);
    }
    
    return 0;
}
